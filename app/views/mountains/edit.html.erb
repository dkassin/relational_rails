
<%= form_with url: "/mountains/#{@mountain.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :volcanic %>
  <%= form.text_field :volcanic %>
  <%= form.label :elevation %>
  <%= form.text_field :elevation %>
  <%= form.submit 'Update Mountain' %>
<% end %>
