<%= form_with url: "/soccer_teams/#{@soccer_teams.id}/soccer_players", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :homegrown_player %>
  <%= form.text_field :homegrown_player %>
  <%= form.label :games_played %>
  <%= form.text_field :games_played %>
  <%= form.submit 'Create Soccer Player' %>
<% end %>
