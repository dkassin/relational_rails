
<%= form_with url: "/soccer_teams", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :qualified_champ_league %>
  <%= form.text_field :qualified_champ_league %>
  <%= form.label :points %>
  <%= form.text_field :points %>
  <%= form.submit 'Create Soccer Team' %>
<% end %>
